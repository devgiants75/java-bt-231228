package b_Chapter01;

// 자바의 입출력 
public class D_IO {

	public static void main(String[] args) {
		// 입출력
//		입력: 콘솔(console)창을 통해서 개발자로부터 입력을 받는 것
//		출력: 콘솔(console)창으로 데이터를 출력하는 것

//		1. 출력
//		System.out.println(출력하고자하는 변수 또는 상수 또는 데이터값);
//		syso 단축키로 자동 완성 가능
		int number = 123;
		System.out.println(number);
	}

}
